  HM             Boolean?                 @default(false, map: "DF__Vacation__HM__0ABD916C")
  ADC            Boolean?                 @default(false, map: "DF__Vacation__ADC__0BB1B5A5")
  Notes          String?                  @db.NVarChar(255)
  SSMA_TimeStamp Unsupported("timestamp")

  @@index([SSN], map: "Vacation$SSN")
  @@ignore
}

model User {
